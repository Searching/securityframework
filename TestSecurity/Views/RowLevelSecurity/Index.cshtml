@model IEnumerable<Security.Identity.PrincipalRowFilterViewModel>
@{
    ViewBag.Title = "RLSs Index";
}
<h2>RLSs Index</h2>
<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.PrincipalName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RowFilterTypeName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RowFilterValue)
        </th>
        <th></th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.PrincipalName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RowFilterTypeName)
            </td>
            <td>
                @Html.DisplayFor(model => item.RowFilterValue)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.Id })
            </td>
        </tr>
    }

</table>